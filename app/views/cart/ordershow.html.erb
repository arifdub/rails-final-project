<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<h1>Your Order Details</h1>
			
			<table class="table table-striped">
				<th>Item Title </th>
					<th>Item price </th>
				<th>Item quantity </th>
			<% @orderitems.each do |order| %>
			
			<tr>
			<td><%= order.title %></td>
			
			<td><%= order.price %></td>
			<td><%= order.quantity %></td>
			
			
			</tr>
			<% end %>
			</table>
		</div>
	</div>
</div>